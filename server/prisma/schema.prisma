// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

// SQLite database
datasource db {
  provider = "sqlite"
  url      = env("SERVERBASEURL")
}

// // PostgreSLQ database
// datasource db {
//   provider = "postgresql"
//   url      = env("SERVERBASEURL")
// }

model Feedback {
  id String @id @default(uuid())

  type       String
  comment    String
  screenshot String?

  @@map("feedbacks")
}
